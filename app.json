{
  "expo": {
    "name": "F_And_B_Pro",
    "slug": "fandbpro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1b7f63"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-notifications",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "We need access to your camera to let you take photos.",
          "microphonePermission": "We need access to your microphone for recording audio in videos."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow F&B_Pro to access your camera",
          "microphonePermission": "AllowF&B_Pro to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "f556f2d6-017a-47e0-bbfa-e5435deba7ee"
      }
    },
    "owner": "dangnam"
  }
}